<Window x:Class="M07_A05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:M07_A05"
        mc:Ignorable="d"
        Title="MainWindow" Height="820" Width="800">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*">
            </ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <Rectangle Grid.Column="1" Grid.Row="0" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="3" Grid.Row="0" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="5" Grid.Row="0" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="7" Grid.Row="0" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="1" Grid.Row="2" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="3" Grid.Row="2" Fill="BurlyWood"></Rectangle>
