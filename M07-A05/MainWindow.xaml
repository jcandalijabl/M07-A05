        <Rectangle Grid.Column="7" Grid.Row="2" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="1" Grid.Row="4" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="3" Grid.Row="4" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="5" Grid.Row="4" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="7" Grid.Row="4" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="1" Grid.Row="6" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="3" Grid.Row="6" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="5" Grid.Row="6" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="7" Grid.Row="6" Fill="BurlyWood"></Rectangle>



        <Rectangle Grid.Column="0" Grid.Row="1" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="2" Grid.Row="1" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="4" Grid.Row="1" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="6" Grid.Row="1" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="0" Grid.Row="3" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="2" Grid.Row="3" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="4" Grid.Row="3" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="6" Grid.Row="3" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="0" Grid.Row="5" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="2" Grid.Row="5" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="4" Grid.Row="5" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="6" Grid.Row="5" Fill="BurlyWood"></Rectangle>

        <Rectangle Grid.Column="0" Grid.Row="7" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="2" Grid.Row="7" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="4" Grid.Row="7" Fill="BurlyWood"></Rectangle>
        <Rectangle Grid.Column="6" Grid.Row="7" Fill="BurlyWood"></Rectangle>









        <Ellipse Grid.Column="1" Grid.Row="0" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="3" Grid.Row="0" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="5" Grid.Row="0" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="7" Grid.Row="0" Fill="Black"></Ellipse>

        <Ellipse Grid.Column="0" Grid.Row="1" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="2" Grid.Row="1" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="4" Grid.Row="1" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="6" Grid.Row="1" Fill="Black"></Ellipse>

        <Ellipse Grid.Column="1" Grid.Row="2" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="3" Grid.Row="2" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="5" Grid.Row="2" Fill="Black"></Ellipse>
        <Ellipse Grid.Column="7" Grid.Row="2" Fill="Black"></Ellipse>




        <Ellipse Name="vermell1" Grid.Column="0" Grid.Row="7" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="2" Grid.Row="7" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="4" Grid.Row="7" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="6" Grid.Row="7" Fill="Red" MouseDown="vermell_1_click"></Ellipse>

        <Ellipse Grid.Column="1" Grid.Row="6" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="3" Grid.Row="6" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="5" Grid.Row="6" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="7" Grid.Row="6" Fill="Red" MouseDown="vermell_1_click"></Ellipse>

        <Ellipse Grid.Column="0" Grid.Row="5" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="2" Grid.Row="5" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="4" Grid.Row="5" Fill="Red" MouseDown="vermell_1_click"></Ellipse>
        <Ellipse Grid.Column="6" Grid.Row="5" Fill="Red" MouseDown="vermell_1_click"></Ellipse>

        
        

    </Grid>

    
    
    
    
    
    
